        "symfony/debug-pack": "^1.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": { "Evirma\\Bundle\\CoreBundle\\": "" }
    },
    "scripts": {
        "auto-scripts": {
